<nav>
  <% if @user_is_authenticated %>
    <ul class="flex justify-between h-10 items-center bg-gray-300">      
      <li class="px-2.5"><%= link_to "Principal Page", "/index" %></li>
      <li class="px-2.5"><%= button_to "Log Out", destroy_user_session_path, method: :delete %></li>
    </ul>      
  <% else %>
    <ul class="flex justify-end h-10 items-center bg-gray-300">      
      <li class="px-2.5"><%= link_to "Sign Up", "/users/sign_up" %><br /></li>
      <li class="px-2.5"><%= link_to "Sign In", "/users/sign_in" %><br /></li>
    </ul>
  <% end %>
</nav>